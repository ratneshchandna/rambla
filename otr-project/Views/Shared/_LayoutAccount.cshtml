@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row-fluid">
    <div class="row-fluid page_title">
        <div class="span12">
            <h1>@ViewBag.AccountPageTitle</h1>
        </div>
    </div>
    <div class="row-fluid">
        <div class="span3">
            Earnings: @ViewBag.UserEarnings.ToString("C")
        </div>
        <div class="span9">
            @RenderBody()
        </div>
    </div>
</div>